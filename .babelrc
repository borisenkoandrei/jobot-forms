{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["babel-plugin-css-modules-transform", {
            "extensions": [".css"],
            "generateScopedName": "[name]__[local]",
            "extractCss": "./lib/styles/index.module.css"
        }],
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
        ]
    ]
}
